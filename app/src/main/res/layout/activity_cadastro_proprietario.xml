<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".activity.Cadastro_proprietario"
    android:background="@color/background">

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/etNome"
        android:layout_marginTop="55dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/txModelo" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nome"
        android:id="@+id/txPlaca"
        android:layout_alignTop="@+id/etNome"
        android:layout_alignParentStart="true"
        android:layout_marginStart="38dp"
        android:textSize="@dimen/abc_action_button_min_width_overflow_material" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="EndereÃ§o"
        android:id="@+id/txModelo"
        android:layout_below="@+id/etNome"
        android:layout_alignStart="@+id/txPlaca"
        android:layout_marginTop="39dp"
        android:textSize="@dimen/abc_action_button_min_width_overflow_material" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Telefone"
        android:id="@+id/txAno"
        android:layout_centerVertical="true"
        android:layout_alignStart="@+id/txModelo"
        android:textSize="@dimen/abc_action_button_min_width_overflow_material" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Data"
        android:id="@+id/txProprietario"
        android:layout_marginTop="31dp"
        android:textSize="@dimen/abc_action_button_min_width_overflow_material"
        android:layout_below="@+id/txAno"
        android:layout_alignStart="@+id/txAno" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/etEndereco"
        android:layout_alignTop="@+id/txModelo"
        android:layout_alignStart="@+id/etNome"
        android:layout_alignParentEnd="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/etTelefone"
        android:layout_alignTop="@+id/txAno"
        android:layout_alignStart="@+id/etEndereco"
        android:layout_alignParentEnd="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/etData"
        android:layout_alignTop="@+id/txProprietario"
        android:layout_alignStart="@+id/etTelefone"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Salvar"
        android:id="@+id/btOKprop"
        android:layout_alignTop="@+id/btAlterarprop"
        android:layout_alignEnd="@+id/txPlaca" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Alterar"
        android:id="@+id/btAlterarprop"
        android:layout_below="@+id/etData"
        android:layout_toEndOf="@+id/txModelo"
        android:layout_marginTop="25dp" />

</RelativeLayout>
