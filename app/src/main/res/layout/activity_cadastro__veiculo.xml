<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".activity.Cadastro_Veiculo">

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/etNome"
        android:layout_marginTop="55dp"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/txPlaca"
        android:layout_marginStart="115dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Placa"
        android:id="@+id/txPlaca"
        android:layout_alignTop="@+id/etNome"
        android:layout_alignParentStart="true"
        android:layout_marginStart="38dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Modelo"
        android:id="@+id/txModelo"
        android:layout_below="@+id/etNome"
        android:layout_alignStart="@+id/txPlaca"
        android:layout_marginTop="39dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ano"
        android:id="@+id/txAno"
        android:layout_centerVertical="true"
        android:layout_alignStart="@+id/txModelo" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Proprietario"
        android:id="@+id/txProprietario"
        android:layout_below="@+id/txAno"
        android:layout_alignStart="@+id/txAno"
        android:layout_marginTop="65dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/etEndereco"
        android:layout_alignTop="@+id/txModelo"
        android:layout_alignStart="@+id/etNome" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/etTelefone"
        android:layout_alignTop="@+id/txAno"
        android:layout_alignStart="@+id/etEndereco" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Salvar"
        android:id="@+id/btOKprop"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/txProprietario"
        android:layout_marginBottom="41dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Alterar"
        android:id="@+id/btAlterarprop"
        android:layout_alignBottom="@+id/btOKprop"
        android:layout_alignStart="@+id/etTelefone" />

    <Spinner
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/spinnerprop"
        android:layout_alignTop="@+id/txProprietario"
        android:layout_toEndOf="@+id/btOKprop" />


</RelativeLayout>
